{
  "name": "frogsystem/metamorphosis",
  "description": "Simple web application framework, build around PSR-7 and container-interop standards.",
  "homepage": "https://github.com/frogsystem/metamorphosis",
  "support": {
    "issues": "https://github.com/frogsystem/metamorphosis/issues",
    "source": "https://github.com/frogsystem/metamorphosis"
  },
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Moritz Kornher",
      "email": "mail@moritzkornher.de"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "zendframework/zend-diactoros": "^1.0.1",
    "aura/router": "^3.0.0-alpha2",
    "league/flysystem": "^1.0.3",
    "frogsystem/spawn": "dev-master as 1.0.x-dev",
    "psr/http-message": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Frogsystem\\Metamorphosis\\": "src/frogsystem/metamorphosis/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.6.6",
    "codacy/coverage": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Frogsystem\\Metamorphosis\\": "test/"
    }
  }
}
